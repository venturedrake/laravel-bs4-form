{
    "name": "venturedrake/laravel-bs4-form",
    "description": "Laravel 5 form wrappers for Bootstrap 4.",
    "keywords": ["laravel", "form", "bootstrap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Drake",
            "email": "andrew@venturedrake.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/config": "^6.0|^7.0",
        "illuminate/session": "^6.0|^7.0",
        "illuminate/support": "^6.0|^7.0",
        "laravelcollective/html": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "VentureDrake\\BootstrapForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "VentureDrake\\BootstrapForm\\BootstrapFormServiceProvider"
            ],
            "aliases": {
                "BootForm": "VentureDrake\\BootstrapForm\\Facades\\BootstrapForm"
            }
        }
    }
}
